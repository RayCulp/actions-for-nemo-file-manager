[Nemo Action]
Active=true
Name=Extract to separate folders using 7Zip
Comment=Extract each selected archive to a separate folder
Exec=<7zip_extract_to_folders/7zip_extract_to_folders.sh %F>
Icon-Name=7zip_extract_to_folders/7zip
Selection=notnone
Extensions=zip;7z;xlsx;docx;pptx;oxt;xpi;rar;
Terminal=true
EscapeSpaces=false
Quote=double
Dependencies=7z;zenity