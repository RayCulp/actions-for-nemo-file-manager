[Nemo Action]
Active=true
Name=Extract archive(s) to folder(s)
Comment=Extract each selected archive to its own folder
Exec=<7zip_extract_to_folders/7zip_extract_to_folders.sh %F>
Icon-Name=7zip_extract_to_folders/7zip
Selection=notnone
Extensions=zip;7z;xlsx;docx;pptx;oxt;xpi;
Terminal=false
EscapeSpaces=false
Quote=double
Dependencies=7z;zenity